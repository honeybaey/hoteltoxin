mixin filter-date-dropdown(name)

  .filter-date-dropdown_wrapper
    label(for='filter-date-dropdown' class='filter-date-dropdown__label')
      p=name
    input(id='filter-date-dropdown' readonly data-range='true' data-multiple-dates-separator=' - ' class='filter-date-dropdown__input' placeholder= 'ДД.ММ.ГГГГ')
    span(class="filter-date-dropdown_icon")