mixin dropdown-default

  .dropdown-default
   .dropdown-default_wrapper
    label(class="dropdown-default__label-style" for="dropdown-default")
      p Dropdown default
    .iqdropdown
      p.iqdropdown-selection
      .iqdropdown-menu
        .iqdropdown-menu-option(data-id="item1")
          div
            p.iqdropdown-item Мужчины
        .iqdropdown-menu-option(data-id="item2")
          div
            p.iqdropdown-item Женщины
        .iqdropdown-menu-option(data-id="item3")
          div
            p.iqdropdown-item Дети





